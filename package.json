{
  "name": "d3-v4-grid",
  "version": "2.0.0",
  "description": "Grid layout for D3 v4",
  "main": "dist/d3-grid.js",
  "jsnext:main": "src/d3-grid.js",
  "scripts": {
    "start": "babel --watch --plugins \"transform-runtime\" src --out-dir ./dist",
    "prepublish": "babel --plugins \"transform-runtime\" src --out-dir ./dist",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "test": "ava"
  },
  "repository": "https://github.com/finnfiddle/d3-v4-grid.git",
  "keywords": [
    "d3",
    "v4",
    "layout",
    "grid"
  ],
  "author": {
    "name": "Finn Fitzsimons",
    "url": "https://github.com/finnfiddle"
  },
  "license": "MIT",
  "dependencies": {
    "d3-array": "^1.2.0",
    "d3-scale": "^1.0.6",
    "its-set": "^1.1.5"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "gulp-eslint": "^2.1.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
